#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int var1;
    int var2;
    scanf("%d\n", &var1);
    scanf("%d\n", &var2);
    printf("%d\n", var1+var2);
    printf("%d\n", var1-var2);
    printf("%d\n", var1*var2);
    printf("%.2f", (float)var1/var2);
    return 0;
}
