#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
        int array[10][10];

        int i, j, m, n, even = 0, odd = 0;
        scanf("%d %d", &m, &n);


        for (i = 0; i < m; ++i) 

        {
                for (j = 0; j < n; ++j)

                {
                     scanf("%d", &array[i][j]);
                     if ((array[i][j] % 2) == 0)
                     {
                         ++even;
                     }
                     else
                         ++odd;
                 }

        }
                for (i = 0; i < m; ++i)
        {
            for (j = 0; j < n; ++j) 
            {
                scanf("%d", &array[i][j]);
            }
        }
        printf("%d \n", odd);
        printf("%d\n", even);    
    return 0;
}
