#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int var1;
    char var2;
    float var3;
    scanf("%d\n",&var1);
    scanf("%c\n",&var2);
    scanf("%f\n",&var3);
    printf("%d\n",var1);
    printf("%c\n",var2);
    printf("%f\n",var3);

    return 0;
}
