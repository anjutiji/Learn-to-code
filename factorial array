#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int fact (int);
int main() 
{
    int n, a[10],i,f;
      scanf("%d\n",&n);
    for (i=0;i<n;i++)
        scanf("%d\n", &a[i]);
      for (i=0;i<n;i++)
      {
          f=fact(a[i]);
          printf("%d\n",f);  
      }
}
   
int fact(int n)
{
    if (n<=1)
        return 1;
    else 
        return n*fact(n-1);

return 0;
}
