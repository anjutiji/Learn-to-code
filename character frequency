#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char str[100], ch;
    int count = 0;

    gets(str);

    scanf("%c", &ch);

    for (int i = 0; str[i] != '\0'; ++i) 
    {
        if (ch == str[i])
        {
            count++;

        }
    }
    if(count==0)
        printf("Element not present.");
    else
     printf("%d", count);
    return 0;
}
