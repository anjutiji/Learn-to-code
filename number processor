#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    int num,sum=0,i;
    scanf("%d\n",&num);
    if (num%2==0)
    {
    for(i=1;i<=2;i++)
    {
        sum=num*num+2;
        num=sum;
    }
    }
    else 
    {
        for(i=1;i<=3;i++)
        {
             sum=num*num+3;
             num=sum;
        }
    }
    
    printf("%d",num);
return 0;
}
