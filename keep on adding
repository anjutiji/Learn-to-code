#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

int num,sum=0,i=1;
    while(1)
    {
        scanf("%d\n",&num);
        if (num==0)
            break;
        sum=sum+num;
        
    if(i>1)
        printf("%d\n",sum);
    i++;
    }
    

    return 0;
}
