#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a[100],n,i;
    printf("Enter the number of elements in the array :\n");
    scanf("%d\n", &n);
    for(i=0;i<n;i++)
    {
        scanf("%d\n", &a[i]);
    }
      
     
     
    for(i=n-1;i>=0;i--)
    {
        printf("%d\n", a[i]);
    }
    
    return 0;
}
